{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/api/*"]
  },

  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/*",
      "allowedRoles": ["authenticated"]
    }
  ],

  "globalHeaders": {
    "Cache-Control": "no-cache"
  }
}